<div id="listContainer"></div>
<script type="text/javascript">
    var listName = "YourListName"; // Replace with your list name
    var siteUrl = _spPageContextInfo.webAbsoluteUrl;

    function renderListView() {
        var ctx = new SP.ClientContext(siteUrl);
        var list = ctx.get_web().get_lists().getByTitle(listName);
        var camlQuery = new SP.CamlQuery();
        var items = list.getItems(camlQuery);

        ctx.load(items);
        ctx.executeQueryAsync(function() {
            var listEnumerator = items.getEnumerator();
            var html = "<table><thead><tr>";

            // Add table headers
            html += "<th>Title</th>"; // Add your column names here
            html += "</tr></thead><tbody>";

            // Add table rows
            while (listEnumerator.moveNext()) {
                var listItem = listEnumerator.get_current();
                html += "<tr>";
                html += "<td>" + listItem.get_fieldValues()["Title"] + "</td>"; // Add your column values here
                html += "</tr>";
            }

            html += "</tbody></table>";
            document.getElementById("listContainer").innerHTML = html;
        }, function(sender, args) {
            document.getElementById("listContainer").innerHTML = "Error: " + args.get_message();
        });
    }

    // Load the SharePoint script and call the function
    SP.SOD.executeFunc('sp.js', 'SP.ClientContext', renderListView);
</script>
